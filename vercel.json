{
    "version": 2,
    "builds": [{
            "src": "src/api/server.js",
            "use": "@vercel/node"
        },{
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "build" }
        }],
        "routes": [
            {
                 "src": "/api/(.*)", "dest": "/api" 
            },{
            "src": "/(.*)",
            "dest": "/$1"
    }]
}