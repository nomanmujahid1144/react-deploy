{
    "version": 2,
    "builds": [
      { "src": "./src/index.tsx", "use": "@vercel/static-build" },
      { "src": "./src/api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/api/$1" },
      { "src": "/(.*)", "dest": "src/index.tsx" }
    ]
  }
  